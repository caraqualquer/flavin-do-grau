local AntiAfk = Configuracoes:CreateButton({
   Name = "AntiAFK",
   Callback = function()
   while true do
    local args = {
        [1] = false
    }
    game:GetService("ReplicatedStorage"):WaitForChild("Assets"):WaitForChild("Remotes"):WaitForChild("AFK"):FireServer(unpack(args))
    wait(0.1) 
end
   end,
})
local Lighting = game:GetService("Lighting")

local Dropdown = Configuracoes:CreateDropdown({
    Name = "Mudar o tempo",
    Options = {"Nenhum", "Dia", "Noite"},
    CurrentOption = {"Nenhum"},
    MultipleOptions = false,
    Flag = "Dropdown1",
    Callback = function(Option)
        local escolha = Option
        if typeof(Option) == "table" then
            escolha = Option[1]
        end

        if escolha == "Dia" then
            Lighting.ClockTime = 12
            Lighting.Brightness = 2
            Lighting.OutdoorAmbient = Color3.fromRGB(200, 200, 200)
            Lighting.Ambient = Color3.fromRGB(180, 180, 180)
        elseif escolha == "Noite" then
            Lighting.ClockTime = 0
            Lighting.Brightness = 0.4
            Lighting.OutdoorAmbient = Color3.fromRGB(50, 50, 80)
            Lighting.Ambient = Color3.fromRGB(70, 70, 100)
        elseif escolha == "Nenhum" then
        end
    end,
})


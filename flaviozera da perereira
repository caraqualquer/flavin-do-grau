local Input = AptidaoFisica:CreateInput({
   Name = "Polichinelos",
   CurrentValue = "",
   PlaceholderText = "Quantidade",
   RemoveTextAfterFocusLost = false,
   Flag = "Input1",
   Callback = function(Text)
      local numero = tonumber(Text)
      if not numero then
         warn("âš ï¸ Digita um nÃºmero vÃ¡lido aÃ­, pÃ´!")
         return
      end

      if numero < 0 then
         warn("âš ï¸ NÃºmero nÃ£o pode ser negativo!")
         return
      end

      local Prefix = " !"
      local Method = "Normal"
      for i = 0, numero do
         local Success, String = Extenso:Convert(i)
         if Success then
            Methods[Method](String, Prefix)
         else
            Methods[Method](tostring(i), Prefix)
         end
         task.wait(Options.Tempo or 0.03)
      end
   end,
})

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--      NOVO INPUT: JJ's 360 e Cx2				
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local function Do360()
   local hrp = LP.Character and LP.Character:FindFirstChild("HumanoidRootPart")
   if hrp then
      local originalCFrame = hrp.CFrame
      -- Faz o pulo
      Char:Jump()
      -- Gira 360 graus no ar
      local startTime = tick()
      local spinDuration = 0.5 -- tempo para girar
      while tick() - startTime < spinDuration do
         hrp.CFrame = hrp.CFrame * CFrame.Angles(0, math.rad(10), 0)
         task.wait()
      end
      -- Volta para posiÃ§Ã£o inicial
      hrp.CFrame = originalCFrame
   end
end

local function waitForDoubleC()
   local cCount = 0
   local lastPress = 0
   local timeout = 1 -- segundos para considerar duplo C
   local finished = false

   local conn
   conn = UserInputService.InputBegan:Connect(function(input, processed)
      if processed then return end
      if input.KeyCode == Enum.KeyCode.C then
         if tick() - lastPress < timeout then
            cCount = cCount + 1
         else
            cCount = 1
         end
         lastPress = tick()

         if cCount >= 2 then
            finished = true
            conn:Disconnect()
         end
      end
   end)

   while not finished do
      task.wait(0.01)
   end
end

local Input360 = AptidaoFisica:CreateInput({
   Name = "Salto PQD",
   CurrentValue = "",
   PlaceholderText = "360",
   RemoveTextAfterFocusLost = false,
   Flag = "Input2",
   Callback = function(Text)
      local numero = tonumber(Text)
      if not numero then
         warn("âš ï¸ Digita um nÃºmero vÃ¡lido aÃ­, pÃ´!")
         return
      end

      if numero < 0 then
         warn("âš ï¸ NÃºmero nÃ£o pode ser negativo!")
         return
      end

      local Prefix = " !"
      local Method = "Normal"

      for i = 0, numero do
         local Success, String = Extenso:Convert(i)
         -- Faz pulo + 360
         Do360()
         -- Espera duplo C
         waitForDoubleC()
         -- SÃ³ envia mensagem apÃ³s tudo
         if Success then
            Methods[Method](String, Prefix)
         else
            Methods[Method](tostring(i), Prefix)
         end
      end
   end,
})

